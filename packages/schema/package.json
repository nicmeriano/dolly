{
  "name": "@dolly/schema",
  "version": "0.1.0",
  "description": "Zod schemas and inferred TypeScript types for dolly plan files",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
